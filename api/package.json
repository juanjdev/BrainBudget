{
  "name": "@brainbudget/api",
  "version": "0.0.1",
  "private": true,
  "description": "BrainBudget API - NestJS Backend",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
